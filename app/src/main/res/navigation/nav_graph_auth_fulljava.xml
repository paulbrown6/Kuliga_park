<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_auth"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.app.kuliga.ui.fragments.LoginFragment"
        android:label="login_fragment_fulljava"
        tools:layout="@layout/login_fragment_fulljava" >
        <action
            android:id="@+id/action_loginFragment_to_registrationFragment"
            app:destination="@id/registrationFragment" />
        <action
            android:id="@+id/action_loginFragment_to_noPassFragment"
            app:destination="@id/noPassFragment" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.app.kuliga.ui.fragments.RegistrationFragment"
        android:label="registration_fragment_fulljava"
        tools:layout="@layout/registration_fragment_fulljava" />
    <fragment
        android:id="@+id/noPassFragment"
        android:name="com.app.kuliga.ui.fragments.NoPassFragment"
        android:label="no_pass_fragment_fulljava"
        tools:layout="@layout/no_pass_fragment_fulljava" />
</navigation>